package suma.de.numeros;

/**
 *
 * @author Usuario
 */
public class SumaDeNumeros {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
   
        int suma = 0;
        for(int i = 1; i <= 100; i++){
            suma = suma + i;
        }
        System.out.println(suma);
    }
}
    
    

