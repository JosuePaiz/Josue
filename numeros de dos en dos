package dosendos;

/**
 *
 * @author josue
 */

public class Dosendos {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args){
        System.out.println("Numeros del 1  a 100 de dos en dos");
       for(int n=1; n<100;n++){
       if(n%2==0)
       System.out.println(n+"");
    }
    }
}
